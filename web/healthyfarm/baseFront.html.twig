            <script type='text/javascript' src='{{ asset('FrontOfficeTemplate/js/custom/_front.min.js') }}'></script>
            <script type='text/javascript' src='{{ asset('FrontOfficeTemplate/js/custom/shortcodes_init.min.js') }}'></script>
            <script type='text/javascript' src='{{ asset('FrontOfficeTemplate/js/custom/_form_contact.js') }}'></script>
            <script type='text/javascript' src='{{ asset('FrontOfficeTemplate/js/custom/__main.js') }}'></script>

            <script type='text/javascript' src='http://maps.google.com/maps/api/js'></script>
            <script type='text/javascript' src='{{ asset('FrontOfficeTemplate/js/custom/_googlemap_init.js') }}'></script>
        {% endblock %}
