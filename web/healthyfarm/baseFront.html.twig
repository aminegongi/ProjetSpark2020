            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/js/vendor/swiper/idangerous.swiper.scrollbar.css') }}' type='text/css' media='all' />
            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/js/vendor/magnific-popup/magnific-popup.css') }}' type='text/css' media='all' />
            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/css/fontello/css/fontello.css') }}' type='text/css' media='all' />
            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/css/fontello/css/animation.css') }}' type='text/css' media='all' />
            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/css/style.css') }}' type='text/css' media='all' />
            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/css/shortcodes.css') }}' type='text/css' media='all' />
            <link rel='stylesheet' href='{{ asset('FrontOfficeTemplate/css/responsive.css') }}' type='text/css' media='all' />
        {% endblock %}
